<main>
  <div class="container">
    <div id="posts">

      <h2>ログイン</h2>
      <div id="post-error">
        <ul>
          <li><%= @error_message %></li>
        </ul>
      </div>
      <%= form_tag(login_url) do %>
        <h3>メールアドレス</h3>
        <input type="text" name="email" value="<%= @email %>">
        <h3>パスワード</h3>
        <input type="password" name="password" value="<%= @password %>">
        <input id="submit-button" type="submit" value="ログイン">
      <% end %>

    </div>
  </div>
</main>
